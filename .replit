run = "dart bin/main.dart"
entrypoint = "bin/main.dart"

[packager]
language = "dart-pub"

[packager.features]
packageSearch = true

[nix]
channel = "stable-22_11"

[languages]

[languages.dart]
pattern = "**/*.dart"

[languages.dart.languageServer]
start = "dart language-server"

[deployment]
run = ["sh", "-c", "dart bin/main.dart"]
